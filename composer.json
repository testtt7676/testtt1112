{
    "name": "example/composer-dc-test",
    "description": "Test configuration for Composer dependency confusion detection",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Test Author",
            "email": "test@example.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "symfony/console": "^6.2",
        "doctrine/orm": "^2.14",
        "guzzlehttp/guzzle": "^7.5",
        "monolog/monolog": "^3.2",
        "this-package-does-not-exist/fake-php-library": "^1.0.0",
        "nonexistent-vendor/vulnerable-composer-package": "^2.1.0",
        "bogus-company/invalid-php-dependency": "^1.5.0",
        "fake-org/composer-vulnerability-test": "dev-main",
        "vulnerable-test/nonexistent-package": "~1.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/var-dumper": "^6.2",
        "fake-dev-tools/vulnerable-phpunit-extension": "^1.0.0",
        "nonexistent-dev/bogus-testing-framework": "^2.0.0",
        "invalid-dev-vendor/fake-dev-package": "dev-develop"
    },
    "suggest": {
        "ext-redis": "For Redis cache support",
        "nonexistent-extension/fake-suggest": "For fake functionality",
        "vulnerable-suggest/bogus-extension": "For testing purposes"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.company-internal.com"
        },
        {
            "type": "vcs",
            "url": "https://github.com/nonexistent-owner/nonexistent-php-repo"
        },
        {
            "type": "package",
            "package": {
                "name": "custom/fake-package",
                "version": "1.0.0",
                "source": {
                    "url": "https://github.com/fake-user/vulnerable-custom-package.git",
                    "type": "git",
                    "reference": "main"
                }
            }
        }
    ],
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "fake-plugin/vulnerable-composer-plugin": true,
            "nonexistent-plugin/bogus-installer": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-install-cmd": [
            "php artisan optimize",
            "composer install-fake-vulnerable-package"
        ],
        "post-update-cmd": [
            "php artisan optimize",
            "fake-post-update-script"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "nonexistent-laravel/fake-service-provider",
                "vulnerable-laravel/bogus-package"
            ]
        }
    }
}